<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="en_marche_core.intl.city" path="/search-city" methods="GET|POST">
        <default key="_controller">EnMarcheCoreBundle:Intl:searchCity</default>
    </route>

    <route id="en_marche_core.intl.insee_code" path="/search-insee-code" methods="GET|POST">
        <default key="_controller">EnMarcheCoreBundle:Intl:searchInseeCode</default>
    </route>

</routes>
