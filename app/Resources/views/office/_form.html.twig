{{ form_row(form.name) }}
{{ form_row(form.regularOpeningHour) }}
{{ form_row(form.regularClosingHour) }}
{{ form_row(form.address.streetNumber) }}
{{ form_row(form.address.streetRepeater) }}
{{ form_row(form.address.streetType) }}
{{ form_row(form.address.streetName) }}
{{ form_row(form.address.countryCode, {'label': 'Pays'}) }}
{{ form_row(form.address.postalCode, {'attr': {'class': 'city-autocomplete'}, 'label': 'Code postal'}) }}
{{ form_row(form.address.cityName, {'attr': {'class': 'city-autocomplete-city-name'}, 'label': 'Ville'}) }}

<div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
        {{ form_row(form.address.inseeCityCode, {'attr': {'class': 'city-autocomplete-insee-code'}}) }}
        {{ form_rest(form) }}
        <input type="submit" class="btn btn-primary">
    </div>
</div>
